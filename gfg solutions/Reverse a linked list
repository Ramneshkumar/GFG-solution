class Solution
{
    public:
    struct Node* reverseList(struct Node *head)
    {
        Node *pre = NULL;
        Node *curr = head;
        Node *next = NULL;
        
        while(curr != NULL)
        {
            next = curr->next;
            curr->next = pre;
            pre = curr;
            curr = next;
        }
        return pre;
    }
    
};
